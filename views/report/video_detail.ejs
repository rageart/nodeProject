                <div class="panel panel-default clearfix mt10 video-detail">
					<div class="panel-body">
						<div class="row">
							<div class="col-xs-6">
								<div class="sub-header">
									<h4>촬영장소</h4>
								</div>
								<div class="module-group clearfix margin_none">
									<div class="map" id="map" style="background:url(/images/cam/map_sample.png)"></div>
								</div>
								<!-- /module-group -->
							</div>
							<!--/col-xs-6 -->
							<div class="col-xs-6">
								<div class="sub-header">
									<h4>썸네일</h4>
								</div>
								<div class="module-group clearfix margin_none">
									<ul class="thumbnail-list">
										<li class="thumbnail-list-item" >
											<img src="/images/cam/image_sample.png" data-title="Image1"/>
										</li>
										<li class="thumbnail-list-item" >
											<img src="/images/cam/image_sample.png" data-title="Image2"/>
										</li>
										<li class="thumbnail-list-item">
											<img src="/images/cam/image_sample.png" data-title="Image3"/>
										</li>
										<li class="thumbnail-list-item">
											<img src="/images/cam/image_sample.png" data-title="Image4"/>
										</li>
									</ul>
									<!--/ ul -->
									<ul class="thumbnail-list">
										<li class="thumbnail-list-item">
											<img src="/images/cam/image_sample.png" data-title="Image5"/>
										</li>
										<li class="thumbnail-list-item">
											<img src="/images/cam/image_sample.png" data-title="Image6"/>
										</li>
										<li class="thumbnail-list-item">
											<img src="/images/cam/image_sample.png" data-title="Image7"/>
										</li>
										<li class="thumbnail-list-item">
											<img src="/images/cam/image_sample.png" data-title="Image8"/>
										</li>
									</ul>
									<!--/ ul -->
								</div>
							</div>
							<!--/col-xs-6 -->
						</div>
						<!--/row -->

						<div class="module-group clearfix">
							<div class="sub-header">
								<h4>동일채널 관제이력</h4>
							</div>
							<div class="grid-wrap mt10">
								<table class="table table-bordered table-condensed table-selecter" summary="동일채널 관제이력">
									<caption class="sr-only">동일채널 관제이력</caption>
									<thead>
										<tr>
											<th scope="col">이름</th>
											<th scope="col">부서명</th>
											<th scope="col">직급</th>
											<th scope="col">관제횟수</th>
											<th scope="col">최초관제시간</th>
